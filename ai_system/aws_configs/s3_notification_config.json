{
    "LambdaFunctionConfigurations": [
        {
            "Id": "smartcondominio-ai-image-processor",
            "LambdaFunctionArn": "arn:aws:lambda:us-east-1:570082719491:function:smartcondominio-ai-processor",
            "Events": [
                "s3:ObjectCreated:*"
            ],
            "Filter": {
                "Key": {
                    "FilterRules": [
                        {
                            "Name": "suffix",
                            "Value": ".jpg"
                        }
                    ]
                }
            }
        },
        {
            "Id": "smartcondominio-ai-image-processor-png",
            "LambdaFunctionArn": "arn:aws:lambda:us-east-1:570082719491:function:smartcondominio-ai-processor",
            "Events": [
                "s3:ObjectCreated:*"
            ],
            "Filter": {
                "Key": {
                    "FilterRules": [
                        {
                            "Name": "suffix",
                            "Value": ".png"
                        }
                    ]
                }
            }
        }
    ]
}